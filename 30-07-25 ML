    "quasi_constant_features = []\n",
    "\n",
    "# Set the threshold (98%)\n",
    "threshold = 0.98\n",
    "\n",
    "for col in x_train.columns:\n",
    "    freq = x_train[col].value_counts(normalize=True).values[0]\n",
    "    if freq >= threshold:\n",
    "        quasi_constant_features.append(col)\n",
    "\n",
    "print( quasi_constant_features)\n",
    "\n"
   ]
